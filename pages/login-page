require '../base-page'
class LoginPage < BasePage
  def initialize
    @browser = driver
  end
  def browser
    return @browser
  end
  def username_textField
    @browser.find_element(:id,'login')
  end
  def password_textField
    @browser.find_element(:id,'password')
  end
  def login_button
    @browser.find_element(:id,'loginbutton')
  end
end